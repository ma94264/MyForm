<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
	>

	<var name="pList" class="java.util.ArrayList"/>
	<view-state id="download" view="form/content">
	    <secured attributes="ROLE_Admin" match="any" />
		<on-entry>
			<evaluate expression="ParticipantDao.getAllParticipants()" result="pList"/>
		</on-entry>
	</view-state>
	
</flow>